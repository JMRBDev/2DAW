{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "outDir": "./public/js",
    "rootDir": "./src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false
  }
}
